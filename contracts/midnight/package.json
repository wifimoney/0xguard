{
    "name": "0xguard/contracts",
    "version": "0.0.1",
    "author": "tinybird",
    "type": "module",
    "main": "./build/index.js",
    "module": "./build/index.js",
    "types": "./build/index.d.ts",
    "exports": {
        ".": {
            "types": "./build/index.d.ts",
            "require": "./build/index.js",
            "import": "./build/index.js",
            "default": "./build/index.js"
        }
    },
    "scripts": {
        "clean": "rm -f build/*.js build/*.d.ts build/witnesses.ts",
        "compact": "compact compile src/AuditVerifier.compact ./build",
        "build": "npm run clean && npm run compact && tsc --project tsconfig.build.json && cp -Rf ./src/witnesses.ts ./build/witnesses.ts",
        "ci": "npm run typecheck && npm run lint && npm run build && npm run test",
        "lint": "eslint src",
        "test": "vitest run",
        "typecheck": "tsc -p tsconfig.json --noEmit"
    },
    "dependencies": {
        "@midnight-ntwrk/compact-runtime": "^0.9.0",
        "@midnight-ntwrk/dapp-connector-api": "^3.0.0",
        "@midnight-ntwrk/ledger": "^4.0.0",
